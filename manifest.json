{
  "name": "Serialforwarder",
  "description": "Forward serial to TCP",
  "version": "0.2",
  "manifest_version": 2,
  "minimum_chrome_version": "40",
  "icons": {
    "128": "icon_128.png"
  },
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "permissions": [
      "serial",
      "webview", "system.network"
  ],
  "sockets": {
      "tcpServer": { "listen" : "" }
  }
}
